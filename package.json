  "devDependencies": {
    "@types/bluebird": "^3.5.4",
    "@types/lodash": "^4.14.68",
    "@types/moment": "^2.13.0",
    "@types/node": "^7.0.22",
    "@types/uuid": "^3.0.0",
    "aws-sdk": "^2.181.0",
    "serverless": "^1.17.0",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-offline": "^3.15.0",
    "serverless-webpack": "2.1.0",
    "ts-loader": "^2.2.2",
    "ts-node": "^3.2.0",
