{
  "name": "serverless-http-starter-kit",
  "version": "1.0.0",
  "description": "A slightly opinionated starter kit for building serverless APIs.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .webpack/ .serverless/",
    "test": "echo \"test harness is not prodived :(\" && exit 1",
    "start": "sls offline start -r <aws-region> -s alpha --acct <account-alias>",
    "deploy:<environment>": "sls deploy -v -r <aws-region>-s <environment> --acct <account-alias>"
  },
